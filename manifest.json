{
  "manifest_version": 2,
  "name": "Shadooowws Extensions",
  "description": "Shadooowws Extensions",
  "version": "1.0",
  "developer": { 
    "name": "Shadooowws",
    "url": "http://twitter.com/Shadooowws"
  },
  "icons": {
    "16": "common/icons/16.png",
    "48": "common/icons/48.png",
    "128": "common/icons/128.png"
  }, 
  "browser_action": {
    "default_icon": {
      "19": "common/icons/19.png",
      "38": "common/icons/38.png"
    },
    "default_popup": "common/html/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ]
}
